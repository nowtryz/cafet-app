{
    "extends": [
        "airbnb"
    ],
    "plugins": [
        "compat"
    ],
    "rules": {
        "semi": [2, "never"],
        "no-unexpected-multiline": 2,
        "compat/compat": 2,
        "react/jsx-one-expression-per-line": 0,
        "function-paren-newline": 0,
        "indent": [2, 4]
    },
    "env": {
        "browser": true
    },
    "settings": {
        "polyfills": ["fetch"]
    }
}