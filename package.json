{
  "name": "cafet-api-php",
  "version": "0.2.2",
  "description": "Both REST API for http connections and PHP librairy. Manage Interactions between client applications and database",
  "private": true,
  "scripts": {
    "api:doc": "redoc-cli bundle openapi.yml -o doc/index.html",
    "prod:build": "rimraf server/dist && webpack -p --progress",
    "dev:start": "rimraf server/dist && cross-env NODE_ENV=development yarn wds",
    "lint:check": "eslint web-client webpack.config.babel.js --ext .js,.jsx",
    "lint:fix": "eslint web-client webpack.config.babel.js --ext .js,.jsx --fix",
    "wds": "webpack-dev-server --progress",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "compile-sass": "node-sass web-client/material-dashboard/assets/scss/material-dashboard-pro-react.scss web-client/material-dashboard/assets/css/material-dashboard-pro-react.css",
    "minify-sass": "node-sass web-client/material-dashboard/assets/scss/material-dashboard-pro-react.scss web-client/material-dashboard/assets/css/material-dashboard-pro-react.min.css --output-style compressed",
    "map-sass": "node-sass web-client/material-dashboard/assets/scss/material-dashboard-pro-react.scss web-client/material-dashboard/assets/css/material-dashboard-pro-react.css --source-map true"
  },
  "repository": {
    "type": "git",
    "url": "git@git.nowtryz.net:cafet-app/php-api-server.git"
  },
  "author": "Nowtryz",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-classes": "^7.4.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@openapitools/openapi-generator-cli": "0.0.7-3.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "file-loader": "^3.0.1",
    "redoc-cli": "^0.8.3",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "axios": "^0.18.0",
    "chartist": "0.10.1",
    "classnames": "^2.2.6",
    "history": "4.7.2",
    "moment": "2.24.0",
    "node-sass": "^4.12.0",
    "nouislider": "13.1.0",
    "perfect-scrollbar": "1.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-big-calendar": "0.20.3",
    "react-bootstrap-sweetalert": "4.4.1",
    "react-chartist": "0.13.3",
    "react-datetime": "2.16.3",
    "react-dom": "^16.8.6",
    "react-google-maps": "9.4.5",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.5.3",
    "react-jvectormap": "0.0.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "4.3.1",
    "react-router-prop-types": "^1.0.4",
    "react-scripts": "2.1.5",
    "react-swipeable-views": "0.13.1",
    "react-table": "6.9.2",
    "react-tagsinput": "3.19.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0"
  },
  "optionalDependencies": {
    "@types/googlemaps": "3.30.16",
    "@types/markerclustererplus": "2.1.33",
    "ajv": "6.9.1",
    "prettier": "1.16.4"
  }
}
