{
	"request_schem": {
		"login_schem": {
			"origin": "cafet_app",
			"version": "1.0",
			"action": "login/logout/updates/changelog",
			"arguments": {
				"email": "test@localhost.loc",
				"password": "pass"
			}
		},
		"fetch_and_get_schem": {
			"origin": "cafet_app",
			"version": "1.0",
			"session_id": "some_id",
			"action": "function_name",
			"arguments": {
				"1rst_arg": "1rst_value",
				"2nd_arg": "2nd_value",
				"3rd_arg": "3rd_value"
			}
		},
		"order": {
			"origin": "cafet_app",
			"version": "1.0",
			"session_id": "some_id",
			"action": "saveOrder",
			"arguments": {
				"client_id": 256,
				"order": [
					{
						"type": "product",
						"id": 65,
						"amount": 4
					},
					{
						"type": "formula",
						"id": 2,
						"amont": 1,
						"products": [
							2,
							16,
							69
						]
					}
				]
			}
		}
	},
	"return_schem": {
		"error_schem": {
			"status": "error",
			"result": {
				"error_code": "01-001",
				"error_type": "the error type",
				"error_message": "some message",
				"additional_message": "additional_message"
			}
		},
		"result_schem": {
			"status": "ok",
			"result": {
				"the_json_object": ""
			}
		}
	}
}